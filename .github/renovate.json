{
  "extends": [
    "config:best-practices",
    ":disableRateLimiting"
  ],
  "enabledManagers": [
    "github-actions",
    "pip_requirements",
    "custom.regex",
    "ansible-galaxy"
  ],
  "ansible-galaxy": {
    "managerFilePatterns": [
      "/^requirements\\.ya?ml$/"
    ]
  },
  "assignees": [
    "pluggero"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^defaults/main\\.yml$"],
      "matchStrings": [
        "python_version:\\s+\"(?<currentValue>[\\d\\.]+)\""
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "python/cpython",
      "versioningTemplate": "semver"
    }
  ]
}
